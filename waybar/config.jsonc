    "custom/recorder": {
        "format": "ï€½ Rec",
        "format-disabled": "ï€½ Off-air", //An empty format will hide the module.
        "return-type": "json",
        "interval": 1,
        "exec": "echo '{\"class\": \"recording\"}'",
        "exec-if": "pgrep wf-recorder",
    },

    "custom/audiorec": {
        "format": "â™¬ Rec",
